#!/usr/bin/env python3
"""
Command line interface to evolve.simulate_evolution
"""


from eee.evolve.simulate_evolution import simulate_evolution
from eee._private.wrap import wrap_function

import sys

def main(argv=None):

    if argv is None:
        argv = sys.argv[1:]

    # Set arg types for args with None as default
    optional_arg_types = {}

    # Wrap and run function
    wrap_function(simulate_evolution,
                  argv=argv,
                  override_arg_defaults={"write_prefix":"eee_sim"},
                  optional_arg_types=optional_arg_types)

if __name__ == "__main__":
    main()
